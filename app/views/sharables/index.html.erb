<div class='row'>
<%= button_to "New", new_sharable_path, :method => "get", style:'width:50px;' %>
<ol>
<%= will_paginate %>
<% @sharables.each do |sharable| %>
<li> <%= link_to sharable.name, sharable_path(sharable) %>
    | <%= link_to "delete", sharable, method: :delete,
                                  data: { confirm: "You sure?" } %>
</li>
<% end %>
<%= will_paginate %>
</ol>
</div>
